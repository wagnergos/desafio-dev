{
  "openapi": "3.0.0",
  "info": {
    "title": "ByCoders - desafio dev",
    "description": "This is an API ByCoders",
    "version": "1.0.0",
    "contact": {
      "email": "wagner.wgos@gmail.com"
    }
  },
  "paths": {
    "/stores": {
      "get": {
        "tags": ["stores"],
        "summary": "List all stores",
        "description": "List all stores",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer"
                      },
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/transactions": {
      "get": {
        "tags": ["transactions"],
        "summary": "List all transactions",
        "description": "List all transactions",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "integer"
                      },
                      "cpf": {
                        "type": "string"
                      },
                      "card": {
                        "type": "string"
                      },
                      "transaction_at": {
                        "type": "string",
                        "example": "2021-10-29T07:48:39.000Z"
                      },
                      "transaction_category_id": {
                        "type": "string"
                      },
                      "category": {
                        "type": "object",
                        "properties": {
                          "name": {
                            "type": "string"
                          },
                          "type": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
